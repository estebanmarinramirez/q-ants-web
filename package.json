    "pinia": "^2.1.7",
    "primevue": "^4.3.4",
    "vue": "^3.5.13",
    "vue-chartjs": "^5.3.0",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.27.0",
    "@lhci/cli": "^0.14.0",
    "@primevue/auto-import-resolver": "^4.3.4",
    "@vitejs/plugin-vue": "^5.2.4",
